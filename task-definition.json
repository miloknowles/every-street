{
  "family": "everystreet-app",
  "containerDefinitions": [
    {
      "name": "everystreet-app-definition",
      "image": "319140110208.dkr.ecr.us-east-1.amazonaws.com/everystreet-app",
      "cpu": 192,
      "memory": 256,
      "entryPoint": ["gunicorn"],
      "command": ["--log-level", "debug", "--bind", "0.0.0.0:5000", "app:app"],
      "portMappings": [
        {
            "containerPort": 5000,
            "hostPort": 0
        }
      ],
      "environment": [
        {
          "name": "AWS_DEFAULT_REGION",
          "value": "us-east-1"
        },
        {
          "name": "FLASK_ENV",
          "value": "development"
        },
        {
          "name": "FLASK_DEBUG",
          "value": "1"
        }
      ],
      "essential": true
    }
  ]
}
